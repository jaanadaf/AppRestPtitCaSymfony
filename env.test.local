# Configuration de la base de données de test
DATABASE_URL="mysql://root:root_password@127.0.0.1:3306/test_database"

# Activation du mode test
APP_ENV=test
APP_DEBUG=1

# Configuration du cache (si vous voulez éviter que Symfony utilise le cache en mode test)
CACHING_ENABLED=false

# Configuration pour l'email (si vous utilisez un service d'email pour vos tests)
MAILER_DSN=smtp://localhost:1025

# API Key (si votre application utilise une clé API pour des tests)
API_KEY="test-api-key-123456"

# Autres configurations spécifiques aux tests
TEST_SECRET="test-secret-value"
